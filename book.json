{
    "title": "苏巴塔",
    "description": "苏巴塔",
    "author": "lsmhq",
    "language": "zh-hans",
    "root": "/pages",
    "links": {
        "sidebar": {
            "Subata官网": "https://www.subata.top/"
        }
    },
    "styles": {
        "website": "styles/custom.css"
    },
    "plugins": [
        "-lunr",
        "-sharing",
        "-search",
        "-favicon",
        "-page-toc",
        "-insert-logo",
        "expandable-chapters",
        "hide-element",
        "search-pro",
        "custom-favicon",
        "edit-link-plus",
        "back-to-top-button",
        "code",
        "splitter",
        "pageview-count",
        "tbfed-pagefooter",
        "sharing-plus",
        "change_girls",
        "sidebar-style",
        "back-to-top-button",
        "popup",
        "theme-lou",
        "copy-code-button",
        "valine"
    ],
    "pluginsConfig": {
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        },
        "hide-element": {
            "elements": [
                ".gitbook-link",
                ".copyright"
            ]
        },
        "favicon": "/image/logo_subata.ico",
        "edit-link-plus": {
            "base": "https://gitee.com/lsmhq/my-wiki/blob/master/pages",
            "label": "编辑此页"
        },
        "change_girls": {
            "time": 5,
            "urls": [
                "/image/logo_subata.ico","/image/logo_subata.ico","/image/logo_subata.ico","/image/logo_subata.ico","/image/logo_subata.ico","/image/logo_subata.ico","/image/logo_subata.ico","/image/logo_subata.ico"
            ]
        },
        "tbfed-pagefooter": {
            "modify_label": "此页最近修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "twitter": false,
            "weibo": false,
            "all": []
        },
        "sidebar-style": {
            "title": "苏巴塔",
            "author": "苏巴塔"
        },
        "valine": {
            "appId": "EvVbrEptndNRxkxHBgq36539-gzGzoHsz",
            "appKey": "A8zorKFVa8nnyiVsVMqN3Cqu"
        },
        "theme-lou": {
            "color": "#2096FF",
            "favicon": "/image/empty.gif",
            "logo": "/image/empty.gif",
            "copyrightLogo": "",
            "autoNumber": "",
            "titleColor": {
                "h1": "#8b008b",
                "h2": "#20b2aa",
                "h3": "#a52a2a"
            },
            "forbidCopy": false,
            "search-placeholder": "",
            "hide-elements": [
                ".summary .gitbook-link", "#vcomments .txt-right"
            ],
            "copyright": {
                "author": "cesi"
            }
        }
    },
    "variables": {
        "themeLou": {
            "nav": [
                {
                    "target": "_blank",
                    "url": "",
