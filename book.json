{
    "title": "Wizard101 电子书",
    "description": "Wizard101 电子书",
    "author": "lsmhq",
    "language": "zh-hans",
    "root": "/pages",
    "links": {
        "sidebar": {
            "Subata官网": "https://www.subata.top/"
        }
    },
    "styles": {
        "website": "styles/custom.css"
    },
    "plugins": [
        "page-toc",
        "expandable-chapters",
        "hide-element",
        "search-pro",
        "insert-logo",
        "custom-favicon",
        "edit-link-plus",
        "back-to-top-button",
        "code",
        "splitter",
        "pageview-count",
        "tbfed-pagefooter",
        "-sharing",
        "sharing-plus",
        "change_girls",
        "sidebar-style",
        "back-to-top-button",
        "popup"
    ],
    "pluginsConfig": {
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        },
        "hide-element": {
            "elements": [
                ".gitbook-link", ".copyright"
            ]
        },
        "favicon": "/image/logo_subata.ico",
        "insert-logo": {
            "url": "/image/empty.gif",
            "style": "background: none; max-height: 80px; min-height: 80px; border-radius: 80px"
        },
        "edit-link-plus": {
            "base": "https://gitee.com/lsmhq/my-wiki/blob/master/pages",
            "label": "编辑此页"
        },
        "change_girls": {
            "time": 5,
            "urls": [
                "/image/logo_subata.ico"
            ]
        },
        "tbfed-pagefooter": {
            "modify_label": "此页最近修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "twitter": false,
            "weibo": false,
            "all": []
        },
        "sidebar-style": {
            "title": "苏巴塔",
            "author": "苏巴塔"
        }
    }
}